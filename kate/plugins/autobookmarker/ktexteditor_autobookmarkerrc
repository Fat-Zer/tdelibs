[autobookmark0]
filetqmask=*.pl;*.pm
flags=1
mimetqmask=text/x-perl
pattern=sub \\w+

[autobookmark1]
filetqmask=*.hh;*.h
flags=1
mimetqmask=text/x-c++hdr
pattern=^class\\s+[^;]+$

[autobookmark2]
filetqmask=*.js;*.html;*.HTML;*.htm;*.HTM;*.xhtml;*.asp
flags=1
mimetqmask=application/x-javascript;text/html
pattern=\\s*function\\s+\\w+

